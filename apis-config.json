{
  "services": [
    {
      "id": "telegram",
      "name": "Telegram Bot",
      "provider": "Telegram",
      "description": "Primary messaging channel. Commands, reports, voice messages.",
      "capabilities": [
        "Send Messages",
        "Voice Messages",
        "Inline Buttons",
        "File Sharing"
      ],
      "health_check": {
        "method": "GET",
        "url": "https://api.telegram.org/bot${TELEGRAM_BOT_TOKEN}/getMe",
        "timeout_ms": 5000,
        "expected_status": 200
      },
      "api_key": {
        "env_var": "TELEGRAM_BOT_TOKEN",
        "last_rotated": null,
        "rotation_warning_days": 90
      }
    },
    {
      "id": "anthropic",
      "name": "Anthropic Claude",
      "provider": "Anthropic API",
      "description": "Primary LLM. Haiku for everyday tasks, Sonnet for complex work, Opus for security and system code.",
      "capabilities": [
        "Haiku 4.5",
        "Sonnet 4.5",
        "Opus 4.5",
        "Opus 4.6 (Guard)"
      ],
      "health_check": {
        "method": "POST",
        "url": "https://api.anthropic.com/v1/messages",
        "headers": {
          "x-api-key": "${ANTHROPIC_API_KEY}",
          "anthropic-version": "2023-06-01",
          "content-type": "application/json"
        },
        "body": "{\"model\":\"claude-haiku-4-5-20251001\",\"max_tokens\":1,\"messages\":[{\"role\":\"user\",\"content\":\"ping\"}]}",
        "timeout_ms": 10000,
        "expected_status": 200
      },
      "api_key": {
        "env_var": "ANTHROPIC_API_KEY",
        "last_rotated": null,
        "rotation_warning_days": 90
      }
    },
    {
      "id": "gemini",
      "name": "Gemini Flash",
      "provider": "Google AI Studio",
      "description": "Free-tier LLM for agent tasks. 250 RPD, 10 RPM limits.",
      "capabilities": [
        "Gemini 2.5 Flash",
        "Free Tier",
        "Multimodal"
      ],
      "health_check": {
        "method": "GET",
        "url": "https://generativelanguage.googleapis.com/v1beta/models?key=${GEMINI_API_KEY}",
        "timeout_ms": 5000,
        "expected_status": 200
      },
      "api_key": {
        "env_var": "GEMINI_API_KEY",
        "last_rotated": null,
        "rotation_warning_days": 90
      }
    },
    {
      "id": "openai",
      "name": "OpenAI / Codex",
      "provider": "OpenAI API",
      "description": "Codex CLI for code generation tasks. Runs under ChatGPT Plus subscription.",
      "capabilities": [
        "GPT-5.3 Codex",
        "Code Generation",
        "Full-Auto Mode"
      ],
      "health_check": {
        "method": "GET",
        "url": "https://api.openai.com/v1/models",
        "headers": {
          "Authorization": "Bearer ${OPENAI_API_KEY}"
        },
        "timeout_ms": 5000,
        "expected_status": 200
      },
      "api_key": {
        "env_var": "OPENAI_API_KEY",
        "last_rotated": null,
        "rotation_warning_days": 90
      }
    },
    {
      "id": "openclaw",
      "name": "OpenClaw Gateway",
      "provider": "Local",
      "description": "Agent gateway managing Jarvis and future agents.",
      "capabilities": [
        "Agent Runtime",
        "Memory",
        "Tools",
        "Cron Jobs"
      ],
      "health_check": {
        "method": "GET",
        "url": "http://localhost:18789/health",
        "timeout_ms": 3000,
        "expected_status": 200
      },
      "api_key": null
    },
    {
      "id": "clawd_control",
      "name": "Clawd Control",
      "provider": "Local",
      "description": "Dashboard and task management API.",
      "capabilities": [
        "Dashboard",
        "Tasks API",
        "Cost Tracking"
      ],
      "health_check": {
        "method": "GET",
        "url": "http://localhost:3100/api/tasks/stats",
        "timeout_ms": 3000,
        "expected_status": 200
      },
      "api_key": null
    },
    {
      "id": "brave_shim",
      "name": "Brave/DDG Search Shim",
      "provider": "Local (DuckDuckGo proxy)",
      "description": "Local proxy emulating Brave Search API via DuckDuckGo. No API key required. Free unlimited web search.",
      "capabilities": [
        "Web Search",
        "Local Search",
        "RAG Compatible"
      ],
      "health_check": {
        "method": "GET",
        "url": "http://localhost:8000/res/v1/web/search?q=ping&count=1",
        "timeout_ms": 5000,
        "expected_status": 200
      },
      "api_key": null
    },
    {
      "id": "camoufox",
      "name": "Camoufox",
      "provider": "Local",
      "description": "Anti-detection Firefox fork for stealth browser automation. Bot detection bypass at C++ engine level.",
      "capabilities": [
        "Stealth Browsing",
        "Bot Detection Bypass",
        "Cookie Import",
        "Form Automation"
      ],
      "health_check": {
        "method": "process_check",
        "process_name": "camoufox",
        "timeout_ms": 1000
      },
      "api_key": null,
      "on_demand": true
    }
  ]
}